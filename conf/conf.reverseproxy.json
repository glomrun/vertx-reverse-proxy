{
    "ssl": {
        "trustStorePath": "ssl/client/server-truststore.jks",
        "trustStorePassword": "password",
        "keyStorePath": "ssl/server/server-keystore.jks",
        "keyStorePassword": "password",
        "symKeyPath": "ssl/server/symKey.key",
        "proxyHttpsPort": 8989
    },
    "defaultService" : "sb",
    "maxPayloadSizeBytes": "512k",
    "serviceDependencies": {
        "dependencies": {
            "auth": {
                "host": "localhost",
                "port": 9000,
                "requestPaths": {
                    "sign": "/sign",
                    "auth": "/authenticate"
                }
            },
            "acl": {
                "host": "localhost",
                "port": 9001,
                "requestPaths": {
                    "manifest": "/verifyAndGetManifest"
                }
            },
            "roles": {
                "host": "localhost",
                "port": 9002,
                "requestPaths": {
                    "roles": "/roles"
                }
            }
        }
    },
    "assets": [
        "ico",
        "png",
        "jpg",
        "jpeg",
        "gif",
        "css",
        "js",
        "txt"
    ],
    "rewriteRules": {
        "sb": {
            "protocol": "http",
            "host": "localhost",
            "port": 8080
        },
        "google": {
            "protocol": "http",
            "host": "google.com",
            "port": 80
        },
        "hn": {
            "protocol": "https",
            "host": "news.ycombinator.com",
            "port": 443
        },
        "boa": {
            "protocol": "https",
            "host": "www.bankofamerica.com",
            "port": 443
        }
    }

}